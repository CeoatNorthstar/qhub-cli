# Database Configuration
# Local: postgresql://user:password@localhost:5432/qhub
# Production: Use Cloudflare D1 connection string
DATABASE_URL=postgresql://qhub:qhub_dev_password@localhost:5432/qhub

# Cloudflare D1 Configuration (Production only)
CLOUDFLARE_ACCOUNT_ID=
CLOUDFLARE_DATABASE_ID=
CLOUDFLARE_API_TOKEN=

# AI Configuration
CLOUDFLARE_AI_TOKEN=75pX0slf0zE2EF6Kf0H-MjauYQosat8-wzqXP0eF

# IBM Quantum Configuration (Optional)
