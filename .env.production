# Production Environment Configuration
# DO NOT COMMIT WITH REAL CREDENTIALS

ENVIRONMENT=production

# Cloudflare D1 Database (Production)
DATABASE_URL=cloudflare://d1/qhub-production
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_DATABASE_ID=your-database-id
CLOUDFLARE_API_TOKEN=your-api-token

# AI Configuration (Production keys)
CLOUDFLARE_AI_TOKEN=production-ai-token-here

# Authentication (CHANGE THESE IN PRODUCTION)
JWT_SECRET=production-secret-change-this-to-random-256-bit-key
TOKEN_EXPIRY_HOURS=24

# API Configuration
API_BASE_URL=https://api.qhub.dev
AUTH_CALLBACK_URL=https://qhub.dev/auth/callback

# Logging
LOG_LEVEL=warn
